    Super+Shift+Y allow-when-locked=true hotkey-overlay-title="Wallpaper: Random" { spawn "sh" "-c" "/home/nutty/.local/bin/random_wall"; }
    Super+Escape hotkey-overlay-title="Toggle Shortcuts Inhibit" { toggle-keyboard-shortcuts-inhibit; }
    Super+Tab hotkey-overlay-title="Toggle Overview" { toggle-overview; }
    Super+W hotkey-overlay-title="Toggle Overview" { toggle-overview; }
